<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/viewModelFlowFragment">


    <fragment
        android:id="@+id/viewModelFlowFragment"
        android:name="com.continuedlearning.flow.completedtasks.useaflow.ViewModelFlowFragment"
        android:label="view_model_flow_fragment"
        tools:layout="@layout/flow_task_fragment" >
        <action
            android:id="@+id/action_viewModelFlowFragment_to_advancedFlow"
            app:destination="@id/advancedFlow" />
    </fragment>
    <fragment
        android:id="@+id/advancedFlow"
        android:name="com.continuedlearning.flow.advanced.ui.AdvancedFlow"
        android:label="advanced_flow_fragment"
        tools:layout="@layout/advanced_flow_fragment" >
        <action
            android:id="@+id/action_advancedFlow_to_viewModelFlowFragment"
            app:destination="@id/viewModelFlowFragment" />
    </fragment>
</navigation>